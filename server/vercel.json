{
  "version": 2,
  "name": "diomy-app",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/ping",
      "dest": "api/index.js"
    },
    {
      "src": "/api/route",
      "dest": "api/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    }
  ]
}